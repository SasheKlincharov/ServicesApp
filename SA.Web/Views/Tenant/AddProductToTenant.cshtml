@model SA.Data.Dto.AddProductToTenant

@{
    var allProducts = Model.AllProducts;
    Layout = "~/Pages/Shared/_Layout.cshtml";

}

@using (Html.BeginForm("AddProductToTenant", "Tenant", FormMethod.Post))
{


    @Html.HiddenFor( x => x.TenantId)
    ;

    @Html.DropDownListFor(model => model.ProductId,allProducts, new { @class= "form-control" })

    <button class="btn btn-primary" type="submit">Save</button>
}
